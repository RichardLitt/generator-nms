{
  "name": "<%= moduleName %>",
  "version": "0.0.0",
  "description": "<%= description %>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/<%= githubUsername %>/<%= moduleName %>.git"
  },
  "homepage": "https://github.com/<%= githubUsername %>/<%= moduleName %>",
  "bugs": "https://github.com/<%= githubUsername %>/<%= moduleName %>/issues",
  "author": {
    "name": "<%= name %>",
    "email": "<%= email %>",
    "url": "<%= humanizedWebsite %>"
  },<% if (cli) { %>
  "bin": "cli.js",<% } %>
  "scripts": {
  	"lint": "standard",
    "test": "standard && ava"
  },
  "keywords": [<% for(var i=0; i<keywords.length; i++) {%>
    "<%= keywords[i] %>",<% } %><% if (cli) { %>
    "cli-app",
    "cli",<% } %>
    ""
  ],
  "dependencies": {<% if (cli) { %>
    "meow": "^3.6.0"
  <% } %>},
  "devDependencies": {
    "standard": "^5.4.1",
    "ava": "^0.9.1"
  },
  "standard": {
    "ignore": [
      "test.js"
    ]
  }
}
